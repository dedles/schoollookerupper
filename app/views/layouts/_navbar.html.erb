<% if user_signed_in? %>
	<ul id="dropdown1" class="dropdown-content">
		<li><%= link_to "Add a School!", new_school_path %></li>
	  <li><%= link_to "Edit Profile", edit_user_registration_path %></li>
	  <li class="divider"></li>
	  <li><%= link_to "Sign Out", destroy_user_session_path, method: :delete %></li>
	</ul>
<% end %>
<nav>
	<div class="nav-wrapper">
		<!-- <a href="/" data-no-turbolink class="brand-logo"><img id="logo" src="https://files.schoollookerupper.com/logo/nuther.png" alt="schoollookerupper logo"></a> -->
		<%= link_to root_path, class: "brand-logo", :data => {:no_turbolink => true } do %>
			 <%= image_tag("https://s3.amazonaws.com/files.schoollookerupper.com/logo/schoollookerupper-logowh.png", id: 'logo') %>
		<% end %>

		<ul id="nav-mobile" class="right hide-on-med-and-down">
			<%# link_to "EDU Depot", root_path, class: "brand-logo" %>
			<li><%= link_to "Contact", pages_contact_path %></li>
			<li><%= link_to "About", pages_about_path %></li>
			<li><%= link_to "Schools Listed", schools_path %></li>
			<% if user_signed_in? %>
				<li><a class="dropdown-button" href="#!" data-activates="dropdown1"><%= current_user.first_name.capitalize %>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<i class="material-icons right">arrow_drop_down</i></a></li>
			<% else %>
				<li><%= link_to "Sign Up", new_user_registration_path %></li>
				<li><%= link_to "Sign In", new_user_session_path %></li>
			<% end %>
		</ul>
	</div>
</nav>

